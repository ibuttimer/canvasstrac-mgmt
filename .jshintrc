{
  "node": true,

  "curly": true,
  "latedef": true,
  "quotmark": true,
  "undef": true,
  "unused": true,
  "trailing": true,
  
  // Custom Globals
  "globals"       : {
    "requirejs": true,
    "define": true,
    "Promise": true,
    "DEBUG": true,
    "angular": true,

    // tests
    "beforeEach": true,
    "afterEach": true,
    "describe": true,
    "it": true,
    "module": true,
    "inject": true
  },

  // Ignored Warnings
  "-W003": true,   // 'x' was used before it was defined
  //"-W013": true,   // Missing space after 'x'. It fires with "function() { }"
  //"-W070": true,   // Extra comma. (it breaks older versions of IE)
  "-W026": true    // Inner functions should be listed at the top of the outer function.  
}
